# NSW FuelCheck API Credentials
# Register at: https://api.nsw.gov.au/Product/Index/22
NSW_API_KEY=your_api_key_here
NSW_API_SECRET=your_api_secret_here

# NSW API OAuth Authorization Header
# Format: Basic base64(API_KEY:API_SECRET)
# Generate: echo -n "API_KEY:API_SECRET" | base64
NSW_AUTH_HEADER=Basic your_base64_encoded_credentials

# Cron Job Security
# Generate a random string: openssl rand -base64 32
CRON_SECRET=your_random_secret_here

# Vercel Edge Config (automatically added by Vercel when you create Edge Config)
# Example: https://edge-config.vercel.com/ecfg_xxxxx?token=xxxxx
EDGE_CONFIG=automatically_added_by_vercel

# Edge Config ID (for write access via API)
# Found in Vercel Dashboard > Storage > Edge Config > Settings
EDGE_CONFIG_ID=ecfg_xxxxx

# Vercel API Token (for write access to Edge Config)
# Create at: https://vercel.com/account/tokens
# Needs scope: Read-Write for Edge Config
VERCEL_TOKEN=your_vercel_token_here

